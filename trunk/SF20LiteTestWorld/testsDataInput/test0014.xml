			<weights>(0.3,0.1,0.05,0.05,0.1,0.1,0.1)</weights>
			<interpolant>Linear</interpolant>
		</SFBitmapPerlinNoiseData>
	</SFBitmapTextureData>
	<SFBitmapTextureData name="PerlinTexture2">
		<SFBitmapPerlinNoiseData name="bitmap">
			<width>200</width>
			<height>200</height>
			<bitmap>OriginalNoise</bitmap>
			<weights>(0.5,0.2,0.15,0.05,0.1,0.1,0.1)</weights>
			<interpolant>Linear</interpolant>
		</SFBitmapPerlinNoiseData>
	</SFBitmapTextureData>
</library>
